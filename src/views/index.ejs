      <script type="text/javascript">
          function consultar() {
              //En este Script se valida que la información tecleada en dos cuadros de texto sea igual,
              //de ser así manda llamar el sitio de Google de no ser así manda llamar el sitio de YouTube.
              //La información de los cuadros de texto se manda a un archivo PHP para ser validados y la
              //respuesta se imprime con una función "echo()"
              
              var texto1 = document.getElementById("txt1").value;
              var texto2 = document.getElementById("txt2").value;
              
              //La ruta (URL) del PHP sería sin carpeta en caso de que se encuentr en el mismo lugar del HTML
              //Los parámetros (DATA) a enviar van separados con el caracter &, en caso de ser texto fijo el que
              //se mande no va entre comillas (var1=texto a mandar 1&var2=texto a mandar 2).
              //
              //Para abrir un documento en blanco y en pestaña adicional se crea una variable (a) y se establecen las
              //propiedades TARGET y HREF, al final se ejecuta el evento click() para mandar llamar la otra página.
              //En caso de querer mandar llamar una página del sitio Web sólo se da la ruta y el nombre del archivo,
              //(a.href = "ruta/nombre_archivo.html")
              
              $.ajax({                                        
                  type: "POST",
                  url: "llenarTabla2.php",
                  data: "var1="+texto1+"&var2="+texto2,
                  success: function(resp){
                      if (resp=="si") {
                          var a = document.createElement("a");
                          a.target = "_blank";
                          a.href = "http://www.google.com.mx";
                          a.click();
                      }
                      else {
                          var a = document.createElement("a");
                          a.target = "_blank";
                          a.href = "http://youtube.com.mx";
                          a.click();
                      }
                  }
              });
              
          }
      </script>

